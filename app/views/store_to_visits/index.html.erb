<p id="notice"><%= notice %></p>

<h1>Listing Store To Visits</h1>

<table>
  <thead>
    <tr>
      <th>Store</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_to_visits.each do |store_to_visit| %>
      <tr>
        <td><%= store_to_visit.store_id %></td>
        <td><%= store_to_visit.user_id %></td>
        <td><%= link_to 'Show', store_to_visit %></td>
        <td><%= link_to 'Edit', edit_store_to_visit_path(store_to_visit) %></td>
        <td><%= link_to 'Destroy', store_to_visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store to visit', new_store_to_visit_path %>
